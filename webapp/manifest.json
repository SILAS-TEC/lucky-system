{
  "_version": "1.12.0",
  "sap.app": {
    "id": "luckyNumbers",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "Lucky Numbers",
    "description": "A simple lucky number generator",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "luckyNumbers.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "luckyNumbers.i18n.i18n"
        }
      },
      "luckModel": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "serviceUrl": "/odata/v4/luck/",
          "synchronizationMode": "None",
          "operationMode": "Server"
        }
      }
    }
  }
}